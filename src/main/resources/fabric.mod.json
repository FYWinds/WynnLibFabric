{
  "schemaVersion": 1,
  "id": "wynnlib",
  "version": "${version}",
  "name": "WynnLibFabric",
  "description": "Item library for Wynncraft",
  "authors": [
    "nbcss",
    "Silentmaxx",
    "FYWind"
  ],
  "contact": {
    "sources": "https://github.com/nbcss/WynnLibFabric"
  },
  "license": "AGPL-3.0-or-later",
  "icon": "assets/wynnlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.nbcss.wynnlib.WynnLibEntry"
      }
    ]
  },
  "mixins": [
    "wynnlib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loaderVersion}",
    "fabric": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "minecraft": "${minecraftVersion}",
    "java": ">=17"
  }
}
